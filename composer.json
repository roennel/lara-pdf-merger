{
    "name": "mobi-market/lara-pdf-merger",
    "description": "",
    "keywords": [
        "merger",
        "merge",
        "pdf",
        "manage",
        "paginate",
        "laravel",
        "duplex merge"
    ],
    "license": [
        "MIT"
    ],
    "authors": [
        {
            "name": "Michael Musso",
            "email": "lynx@deltaaskii.com",
            "homepage": "https://github.com/LynX39"
        },
        {
            "name": "Ramon Smit",
            "email": "rsmit@daltcore.com",
            "homepage": "https://github.com/daltcore"
        },
        {
            "name": "Marouan Sami",
            "email": "sami.marwen.isetcom@gmail.com",
            "homepage": "https://github.com/MarwenSami"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "mobi-market/tcpdi": "^1.0",
        "illuminate/support": ">=5.8"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "homepage": "https://github.com/MarwenSami/lara-pdf-merger",
    "autoload": {
        "psr-0": {
            "LynX39\\LaraPdfMerger": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LynX39\\LaraPdfMerger\\PdfMergerServiceProvider"
            ],
            "aliases": {
                "PdfMerger": "LynX39\\LaraPdfMerger\\Facades\\PdfMerger"
            }
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
